{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "signature": true
  },
  "tasks": {
    "build:skills:exec": {
      "dependsOn": ["generators:build"],
      "outputs": [
        "content/skills/*.zip",
        "apps/web/public/downloads/skills/*.zip"
      ],
      "inputs": [
        "content/skills/*.json",
        "content/skills/*.zip",
        "packages/generators/src/**/*",
        "packages/web-runtime/src/transformers/skill-to-md.ts",
        ".build-cache/cache.json"
      ],
      "cache": true,
      "persistent": true,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "generate:service-worker:exec": {
      "dependsOn": ["generators:build"],
      "outputs": ["apps/web/public/service-worker.js"],
      "inputs": [
        "packages/generators/src/**/*",
        "packages/web-runtime/src/data/config/category/category-config.ts",
        "apps/web/src/lib/data/config/constants.ts",
        "apps/web/src/lib/data/config/constants.server.ts",
        "package.json"
      ],
      "cache": true
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": [
        "apps/web/**/*",
        "packages/**/src/**/*",
        "packages/**/package.json",
        "package.json",
        "pnpm-lock.yaml",
        "next.config.mjs",
        "tsconfig.json"
      ],
      "dependsOn": [
        "^build",
        "generate:service-worker:exec",
        "type-check:build",
        "lint:build"
      ]
    },
    "lint:build": {
      "outputs": [],
      "inputs": [
        "packages/web-runtime/src/data/**/*",
        "!packages/web-runtime/src/data/**/*.test.ts",
        "!packages/web-runtime/src/data/**/*.test.tsx",
        "apps/web/src/app/**/*",
        "!apps/web/src/app/**/*.test.ts",
        "!apps/web/src/app/**/*.test.tsx"
      ],
      "cache": true
    },
    "type-check": {
      "outputs": ["**/*.tsbuildinfo"],
      "cache": true
    },
    "type-check:build": {
      "outputs": ["**/*.tsbuildinfo"],
      "inputs": [
        "apps/web/**/*",
        "!apps/web/**/*.test.ts",
        "!apps/web/**/*.test.tsx",
        "!apps/web/**/*.spec.ts",
        "!apps/web/**/*.spec.tsx",
        "packages/web-runtime/src/**/*",
        "!packages/web-runtime/src/**/*.test.ts",
        "!packages/web-runtime/src/**/*.test.tsx",
        "packages/data-layer/src/**/*",
        "!packages/data-layer/src/**/*.test.ts",
        "!packages/data-layer/src/**/*.test.tsx",
        "packages/database-types/src/**/*",
        "!packages/database-types/src/**/*.test.ts",
        "!packages/database-types/src/**/*.test.tsx",
        "packages/shared-runtime/src/**/*",
        "!packages/shared-runtime/src/**/*.test.ts",
        "!packages/shared-runtime/src/**/*.test.tsx",
        "tsconfig.json"
      ],
      "cache": true
    },
      "type-check:web": {
        "outputs": [],
        "inputs": [
          "apps/web/**/*",
          "packages/**/src/**/*",
          "tsconfig.json"
        ],
        "cache": true,
        "dependsOn": ["^generate:categories:exec"]
      },
      "type-check:packages": {
        "outputs": [],
        "inputs": [
          "packages/**/src/**/*",
          "packages/**/tsconfig.json"
        ],
        "cache": true
      },
      "type-check:edge": {
        "outputs": [],
        "inputs": [
          "apps/edge/functions/**/*",
          "apps/edge/functions/tsconfig.json",
          "apps/edge/functions/deno.json"
        ],
        "cache": true
      },
    "lint": {
      "outputs": [],
      "cache": true
    },
      "lint:edge": {
        "outputs": [],
        "inputs": [
          "apps/edge/functions/**/*"
        ],
        "cache": true
      },
    "test:unit": {
      "outputs": ["coverage/**"],
      "cache": true
    },
    "generators:build": {
      "outputs": ["dist/**"],
      "inputs": [
        "packages/generators/src/**/*",
        "packages/generators/tsconfig.json",
        "package.json"
      ],
      "cache": true,
      "dependsOn": ["^build"]
    },
    "@heyclaude/web-runtime#build": {
      "outputs": ["dist/**/*.d.ts", "dist/**/*.d.ts.map"],
      "inputs": ["src/**/*", "tsconfig.json"],
      "cache": true,
      "dependsOn": ["^build"]
    },
    "@heyclaude/data-layer#build": {
      "outputs": ["dist/**/*.d.ts", "dist/**/*.d.ts.map"],
      "inputs": ["src/**/*", "tsconfig.json"],
      "cache": true,
      "dependsOn": ["^build"]
    },
    "@heyclaude/shared-runtime#build": {
      "outputs": ["dist/**/*.d.ts", "dist/**/*.d.ts.map"],
      "inputs": ["src/**/*", "tsconfig.json"],
      "cache": true,
      "dependsOn": ["^build"]
    },
    "dev:exec": {
      "dependsOn": ["generate:service-worker:exec"],
      "cache": false
    },
    "build:mcpb:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "verify:mcpb:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "generate:readme:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "sync:db:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL", "SUPABASE_PROJECT_REF"]
    },
    "sync:db:full:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL", "SUPABASE_PROJECT_REF"]
    },
    "sync:db:types:exec": {
      "dependsOn": ["generators:build"],
      "outputs": ["packages/database-types/src/**/*"],
      "cache": true,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL", "SUPABASE_PROJECT_REF"]
    },
    "backup:db:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "cache:info:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "cache:clear:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "webhooks:backlog:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "webhooks:replay:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "changelog:generate:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "changelog:generate:dry:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "changelog:generate:branch:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "type-check:unused:filtered:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "verify:routes:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "email:upload:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["RESEND_API_KEY"]
    },
    "deploy:edge:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_PROJECT_REF", "NEXT_PUBLIC_SUPABASE_URL"]
    }
  },
  "globalDependencies": ["package.json", "pnpm-lock.yaml"],
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY"
  ],
  "globalPassThroughEnv": [
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_DEPLOYMENT_ID",
    "VERCEL_GIT_COMMIT_SHA",
    "VERCEL_GIT_COMMIT_MESSAGE",
    "VERCEL_GIT_COMMIT_AUTHOR_LOGIN",
    "VERCEL_GIT_COMMIT_AUTHOR_NAME",
    "VERCEL_GIT_REPO_SLUG",
    "VERCEL_GIT_REPO_OWNER",
    "VERCEL_GIT_REPO_ID",
    "VERCEL_GIT_PULL_REQUEST_ID",
    "TURBO_REMOTE_CACHE_SIGNATURE_KEY",
    "CI",
    "CONTINUOUS_INTEGRATION",
    "NEXT_PUBLIC_SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_KEY"
  ]
}
