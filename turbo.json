{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "signature": true
  },
  "tasks": {
    "build:skills:exec": {
      "dependsOn": ["generators:build"],
      "outputs": ["content/skills/*.zip", "apps/web/public/downloads/skills/*.zip"],
      "inputs": [
        "content/skills/*.json",
        "content/skills/*.zip",
        "packages/generators/src/**/*",
        "packages/web-runtime/src/transformers/skill-to-md.ts",
        ".build-cache/cache.json"
      ],
      "cache": true,
      "persistent": true,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "generate:service-worker:exec": {
      "outputs": ["apps/web/public/service-worker.js"],
      "inputs": [
        "packages/generators/src/commands/generate-service-worker.ts",
        "packages/generators/src/bin/generate-service-worker.ts",
        "packages/generators/src/toolkit/cache.ts",
        "packages/generators/src/toolkit/logger.ts",
        "packages/generators/templates/service-worker.template.js",
        "packages/web-runtime/src/data/config/category/category-config.ts",
        "packages/shared-runtime/src/config/security.ts",
        "package.json"
      ],
      "cache": true
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": [
        "apps/web/src/**/*.{ts,tsx,css}",
        "apps/web/public/**/*",
        "apps/web/next.config.mjs",
        "apps/web/tsconfig.json",
        "apps/web/package.json",
        "packages/**/src/**/*.{ts,tsx}",
        "packages/**/package.json",
        "packages/**/tsconfig.json",
        "!packages/generators/**",
        "!packages/edge-runtime/**",
        "!**/*.test.{ts,tsx}",
        "!**/*.spec.{ts,tsx}",
        "package.json",
        "pnpm-lock.yaml",
        "tsconfig.json"
      ],
      "dependsOn": ["^build", "generate:service-worker:exec"]
    },
    "lint:build": {
      "outputs": [],
      "inputs": [
        "packages/web-runtime/src/data/**/*",
        "!packages/web-runtime/src/data/**/*.test.ts",
        "!packages/web-runtime/src/data/**/*.test.tsx",
        "apps/web/src/app/**/*",
        "!apps/web/src/app/**/*.test.ts",
        "!apps/web/src/app/**/*.test.tsx"
      ],
      "cache": true
    },
    "type-check": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": true,
      "interactive": false
    },
    "type-check:edge": {
      "outputs": [],
      "inputs": ["apps/edge/functions/**/*", "apps/edge/supabase/deno.json"],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "@heyclaude/database-types#build",
        "@heyclaude/shared-runtime#build",
        "@heyclaude/data-layer#build",
        "@heyclaude/web-runtime#build"
      ],
      "outputs": [],
      "cache": true
    },
    "lint:edge": {
      "outputs": [],
      "inputs": ["apps/edge/functions/**/*"],
      "cache": true
    },
    "test:unit": {
      "outputs": ["coverage/**"],
      "cache": true
    },
    "generators:build": {
      "outputs": ["packages/generators/dist/**", "packages/generators/.tsbuildinfo"],
      "inputs": [
        "packages/generators/src/**/*",
        "packages/generators/tsconfig.json",
        "packages/generators/package.json"
      ],
      "cache": true,
      "dependsOn": [
        "@heyclaude/database-types#build",
        "@heyclaude/shared-runtime#build",
        "@heyclaude/data-layer#build",
        "@heyclaude/web-runtime#build"
      ]
    },
    "@heyclaude/web-runtime#build": {
      "outputs": ["dist/**", ".tsbuildinfo"],
      "inputs": ["src/**/*", "tsconfig.json", "package.json"],
      "cache": true,
      "dependsOn": [
        "@heyclaude/database-types#build",
        "@heyclaude/shared-runtime#build",
        "@heyclaude/data-layer#build"
      ]
    },
    "@heyclaude/data-layer#build": {
      "outputs": ["dist/**", ".tsbuildinfo"],
      "inputs": ["src/**/*", "tsconfig.json", "package.json"],
      "cache": true,
      "dependsOn": ["@heyclaude/database-types#build", "@heyclaude/shared-runtime#build"]
    },
    "@heyclaude/shared-runtime#build": {
      "outputs": ["dist/**", ".tsbuildinfo"],
      "inputs": ["src/**/*", "tsconfig.json", "package.json"],
      "cache": true,
      "dependsOn": ["@heyclaude/database-types#build"]
    },
    "@heyclaude/database-types#build": {
      "outputs": ["dist/**", ".tsbuildinfo"],
      "inputs": ["src/**/*", "tsconfig.json", "package.json"],
      "cache": true
    },
    "dev:exec": {
      "dependsOn": ["generate:service-worker:exec"],
      "cache": false
    },
    "build:mcpb:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "verify:mcpb:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "generate:readme:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "sync:db:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": [
        "SUPABASE_SERVICE_ROLE_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "SUPABASE_PROJECT_REF"
      ]
    },
    "sync:db:full:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": [
        "SUPABASE_SERVICE_ROLE_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "SUPABASE_PROJECT_REF"
      ]
    },
    "sync:db:types:exec": {
      "dependsOn": ["generators:build"],
      "outputs": ["packages/database-types/src/**/*"],
      "cache": true,
      "passThroughEnv": [
        "SUPABASE_SERVICE_ROLE_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "SUPABASE_PROJECT_REF"
      ]
    },
    "backup:db:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "cache:info:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "cache:clear:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "webhooks:backlog:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "webhooks:replay:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "changelog:generate:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "changelog:generate:dry:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "changelog:generate:branch:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "NEXT_PUBLIC_SUPABASE_URL"]
    },
    "type-check:unused:filtered:exec": {
      "dependsOn": ["generators:build"],
      "cache": false
    },
    "email:upload:exec": {
      "dependsOn": ["generators:build"],
      "cache": false,
      "passThroughEnv": ["RESEND_API_KEY"]
    },
    "deploy:edge:exec": {
      "cache": false,
      "passThroughEnv": ["SUPABASE_PROJECT_REF", "NEXT_PUBLIC_SUPABASE_URL"]
    }
  },
  "globalDependencies": ["package.json", "pnpm-lock.yaml"],
  "globalEnv": [
    "NODE_ENV",
    "VERCEL_ENV",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY"
  ],
  "globalPassThroughEnv": [
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_DEPLOYMENT_ID",
    "VERCEL_GIT_COMMIT_SHA",
    "VERCEL_GIT_COMMIT_MESSAGE",
    "VERCEL_GIT_COMMIT_AUTHOR_LOGIN",
    "VERCEL_GIT_COMMIT_AUTHOR_NAME",
    "VERCEL_GIT_REPO_SLUG",
    "VERCEL_GIT_REPO_OWNER",
    "VERCEL_GIT_REPO_ID",
    "VERCEL_GIT_PULL_REQUEST_ID",
    "TURBO_REMOTE_CACHE_SIGNATURE_KEY",
    "CI",
    "CONTINUOUS_INTEGRATION",
    "NEXT_PUBLIC_SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_KEY",
    "SUPABASE_ANON_KEY",
    "SUPABASE_JWT_SECRET",
    "POSTGRES_URL",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_PASSWORD",
    "RATE_LIMIT_SECRET",
    "CACHE_WARM_AUTH_TOKEN",
    "VIEW_COUNT_SALT",
    "WEBHOOK_SECRET",
    "GITHUB_TOKEN",
    "GITHUB_REPO_OWNER",
    "GITHUB_REPO_NAME",
    "GITHUB_BRANCH",
    "RESEND_API_KEY",
    "RESEND_AUDIENCE_ID",
    "RESEND_FROM_EMAIL",
    "RESEND_WEBHOOK_SECRET",
    "CRON_SECRET",
    "BETTERSTACK_HEARTBEAT_WEEKLY_TASKS",
    "BETTERSTACK_HEARTBEAT_DAILY_MAINTENANCE",
    "REVALIDATE_SECRET",
    "R2_ACCESS_KEY_ID",
    "R2_SECRET_ACCESS_KEY",
    "R2_ENDPOINT",
    "R2_BUCKET_NAME",
    "INNGEST_EVENT_KEY",
    "INNGEST_SIGNING_KEY",
    "DISCORD_ERROR_WEBHOOK_URL",
    "POLAR_WEBHOOK_SECRET",
    "DISCORD_CHANGELOG_WEBHOOK_URL",
    "DISCORD_ANNOUNCEMENTS_WEBHOOK_URL",
    "DISCORD_SUBMISSIONS_WEBHOOK_URL",
    "DISCORD_JOBS_WEBHOOK_URL"
  ]
}
