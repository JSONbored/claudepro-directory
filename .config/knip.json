{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreDependencies": [
    "sharp",
    "tailwindcss",
    "@biomejs/biome",
    "@asteasolutions/zod-to-openapi",
    "postcss"
  ],
  "ignoreExportsUsedInFile": true,
  "ignore": [
    "components/ui/**",
    "lib/analytics/**",
    "lib/structured-data/**",
    "lib/constants.ts",
    "lib/icons.tsx",
    "lib/ui-constants.ts",
    "data/jobs.ts",
    "components/ui/sonner.tsx",
    "public/js/offline.js",
    "public/scripts/service-worker-init.js",
    "public/service-worker.js"
  ],
  "ignoreBinaries": [
    "skott",
    "madge"
  ],
  "entry": [
    "app/**/*.{ts,tsx}",
    "middleware.ts",
    "scripts/**/*.ts"
  ],
  "project": [
    "**/*.{ts,tsx}"
  ]
}
