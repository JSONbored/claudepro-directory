{
  "name": "@heyclaude/shared-runtime",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./schemas/env": {
      "development": "./src/schemas/env.ts",
      "types": "./dist/schemas/env.d.ts",
      "import": "./dist/schemas/env.js",
      "require": "./dist/schemas/env.js",
      "default": "./dist/schemas/env.js"
    },
    "./privacy.ts": {
      "development": "./src/privacy.ts",
      "types": "./dist/privacy.d.ts",
      "import": "./dist/privacy.js",
      "require": "./dist/privacy.js",
      "default": "./dist/privacy.js"
    },
    "./logger/config.ts": {
      "development": "./src/logger/config.ts",
      "types": "./dist/logger/config.d.ts",
      "import": "./dist/logger/config.js",
      "require": "./dist/logger/config.js",
      "default": "./dist/logger/config.js"
    },
    "./logger/index.ts": {
      "development": "./src/logger/index.ts",
      "types": "./dist/logger/index.d.ts",
      "import": "./dist/logger/index.js",
      "require": "./dist/logger/index.js",
      "default": "./dist/logger/index.js"
    },
    "./error-handling.ts": {
      "development": "./src/error-handling.ts",
      "types": "./dist/error-handling.d.ts",
      "import": "./dist/error-handling.js",
      "require": "./dist/error-handling.js",
      "default": "./dist/error-handling.js"
    },
    "./rate-limit": {
      "development": "./src/rate-limit.ts",
      "types": "./dist/rate-limit.d.ts",
      "import": "./dist/rate-limit.js",
      "require": "./dist/rate-limit.js",
      "default": "./dist/rate-limit.js"
    },
    "./proxy/guards": {
      "development": "./src/proxy/guards.ts",
      "types": "./dist/proxy/guards.d.ts",
      "import": "./dist/proxy/guards.js",
      "require": "./dist/proxy/guards.js",
      "default": "./dist/proxy/guards.js"
    },
    "./env": {
      "development": "./src/env.ts",
      "types": "./dist/env.d.ts",
      "import": "./dist/env.js",
      "require": "./dist/env.js",
      "default": "./dist/env.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "type-check": "tsc --noEmit",
    "lint": "eslint --config eslint.config.mjs src --ignore-pattern '**/*.generated.ts' --ignore-pattern '**/*.test.ts' --ignore-pattern '**/*.spec.ts'",
    "lint:fix": "eslint --config eslint.config.mjs src --ignore-pattern '**/*.generated.ts' --ignore-pattern '**/*.test.ts' --ignore-pattern '**/*.spec.ts' --fix"
  },
  "devDependencies": {
    "@types/sanitize-html": "^2.16.0",
    "esbuild": "^0.27.0",
    "pino-pretty": "^13.1.2"
  },
  "dependencies": {
    "@heyclaude/database-types": "workspace:*",
    "@supabase/supabase-js": "^2.86.2",
    "pino": "^10.1.0",
    "sanitize-html": "^2.17.0",
    "sugar-high": "^0.9.5",
    "zod": "^4.1.12"
  }
}
