{
  "name": "claudepro-directory",
  "private": true,
  "sideEffects": false,
  "version": "1.1.0",
  "description": "A community-driven directory of Claude configurations including agents, MCP servers, rules, commands, and hooks",
  "author": "JSONbored",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JSONbored/claudepro-directory"
  },
  "keywords": [
    "claude",
    "ai",
    "mcp",
    "agents",
    "configurations",
    "directory"
  ],
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "turbo run generate:service-worker:exec && next dev --turbopack",
    "build": "turbo run generate:categories:exec generate:service-worker:exec build:content:exec type-check && next build --turbopack",
    "build:analyze": "TURBOPACK_STATS=1 next build --turbopack",
    "build:skills": "turbo run build:skills:exec",
    "build:skills:exec": "tsx scripts/build/generate-skill-packages.ts",
    "generate:categories": "turbo run generate:categories:exec",
    "generate:categories:exec": "tsx scripts/build/generate-category-config.ts",
    "generate:service-worker": "turbo run generate:service-worker:exec",
    "generate:service-worker:exec": "tsx scripts/build/generate-service-worker.ts",
    "generate:readme": "tsx scripts/build/generate-readme.ts",
    "sync:db": "tsx scripts/build/sync-database-schema.ts",
    "sync:db:force": "tsx scripts/build/sync-database-schema.ts --force",
    "sync:db:types": "tsx scripts/build/sync-database-schema.ts --only-types",
    "sync:db:zod": "tsx scripts/build/sync-database-schema.ts --only-zod",
    "sync:db:dump": "tsx scripts/build/sync-database-schema.ts --only-dump",
    "backup:db": "tsx scripts/build/backup-database.ts",
    "changelog:generate": "tsx scripts/changelog/generate-entry.ts",
    "changelog:generate:dry": "tsx scripts/changelog/generate-entry.ts --dry-run",
    "changelog:generate:branch": "tsx scripts/changelog/generate-entry.ts --branch",
    "indexnow:submit": "tsx scripts/utilities/submit-indexnow.ts",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "ultracite check",
    "format": "ultracite fix",
    "email:dev": "email dev --dir src/emails --port 3001",
    "validate": "tsx scripts/validation/validate.ts",
    "validate:all": "tsx scripts/validation/validate.ts",
    "validate:quick": "tsx scripts/validation/validate.ts --quick",
    "validate:fix": "tsx scripts/validation/validate.ts --fix",
    "validate:content": "tsx scripts/validation/validate.ts --type=content",
    "validate:seo": "tsx scripts/validation/validate.ts --type=seo",
    "validate:seo:fix": "tsx scripts/validation/validate.ts --type=seo --fix",
    "validate:titles": "tsx scripts/validation/validate.ts --type=titles",
    "validate:titles:quick": "tsx scripts/validation/validate.ts --type=titles --quick",
    "validate:llmstxt": "tsx scripts/validation/validate.ts --type=llmstxt",
    "validate:metadata": "tsx scripts/validation/validate.ts --type=metadata",
    "validate:metadata:quick": "tsx scripts/validation/validate.ts --type=metadata --quick",
    "prepare": "command -v lefthook >/dev/null 2>&1 && LEFTHOOK_CONFIG=config/tools/lefthook.yml lefthook install && rm -f lefthook.yml || echo 'Skipping lefthook install (not available)'",
    "analyze:architecture": "npx skott --displayMode=webapp",
    "analyze:unused": "npx knip -c config/tools/knip.json",
    "analyze:circular": "npx madge --circular --extensions ts,tsx src/",
    "analyze:circular-graph": "npx madge --circular --extensions ts,tsx --image circular-deps.svg src/",
    "analyze:all": "npm run analyze:unused && npm run analyze:circular",
    "a11y:audit": "lhci autorun --config=config/tools/lighthouserc.cjs",
    "a11y:ci": "lhci autorun --config=config/tools/lighthouserc.cjs --assert",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --config=config/tools/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=config/tools/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config=config/tools/playwright.config.ts --debug",
    "test:e2e:headed": "playwright test --config=config/tools/playwright.config.ts --headed",
    "test:e2e:chromium": "playwright test --config=config/tools/playwright.config.ts --project=chromium",
    "test:e2e:mobile": "playwright test --config=config/tools/playwright.config.ts --project=mobile-chrome",
    "test:e2e:a11y": "playwright test --config=config/tools/playwright.config.ts --project=accessibility",
    "test:e2e:seo": "playwright test --config=config/tools/playwright.config.ts tests/e2e/seo/comprehensive-sitemap-coverage.spec.ts --project=chromium --reporter=./config/tools/progress-reporter.ts --reporter=html 2>/dev/null",
    "test:e2e:metadata": "playwright test --config=config/tools/playwright.config.ts tests/e2e/seo/sitemap-metadata-validation.spec.ts --project=chromium --reporter=list",
    "test:e2e:clean": "rm -rf playwright-report test-results",
    "test:e2e:report": "playwright show-report playwright-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:all": "npm run test:unit && npm run test:e2e",
    "tech-debt:baseline": "config/tools/tech-debt/tracker.sh baseline",
    "tech-debt:check": "config/tools/tech-debt/tracker.sh check",
    "tech-debt:report": "config/tools/tech-debt/tracker.sh report",
    "admin:submissions:list": "tsx scripts/admin/review-submissions.ts list",
    "admin:submissions:list:pending": "tsx scripts/admin/review-submissions.ts list pending",
    "admin:submissions:list:approved": "tsx scripts/admin/review-submissions.ts list approved",
    "admin:submissions:show": "tsx scripts/admin/review-submissions.ts show",
    "admin:submissions:approve": "tsx scripts/admin/review-submissions.ts approve",
    "admin:submissions:reject": "tsx scripts/admin/review-submissions.ts reject",
    "admin:submissions:spam": "tsx scripts/admin/review-submissions.ts spam",
    "admin:submissions:merge": "tsx scripts/admin/review-submissions.ts merge",
    "admin:submissions:bulk-approve": "tsx scripts/admin/review-submissions.ts bulk-approve",
    "admin:content:create": "tsx scripts/admin/create-content.ts",
    "audit:tracking": "tsx scripts/audit/verify-copy-download-tracking.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/he": "^1.2.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "devalue": "^5.4.2",
    "dompurify": "^3.3.0",
    "embla-carousel-react": "^8.6.0",
    "he": "^1.2.0",
    "jsdom": "^27.1.0",
    "lucide-react": "^0.546.0",
    "marked": "^16.4.1",
    "modern-screenshot": "^4.6.6",
    "motion": "^12.23.24",
    "next": "16.0.0",
    "next-safe-action": "^8.0.11",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.66.0",
    "react-share": "^5.2.2",
    "resend": "^6.4.0",
    "sonner": "^2.0.7",
    "sugar-high": "^0.9.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "type-fest": "^5.1.0",
    "typescript": "5.9.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "2.2.7",
    "@faker-js/faker": "^10.1.0",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.0.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/archiver": "^6.0.4",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.6",
    "archiver": "^7.0.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "fast-xml-parser": "^5.3.0",
    "fishery": "^2.3.1",
    "happy-dom": "^20.0.10",
    "jest-axe": "^10.0.0",
    "knip": "^5.66.4",
    "lefthook": "^2.0.2",
    "msw": "^2.11.6",
    "npm-run-all": "^4.1.5",
    "ora": "^8.2.0",
    "prettier": "^3.6.2",
    "supazod": "^4.1.0",
    "turbo": "^2.6.0",
    "ultracite": "^5.6.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6"
  },
  "pnpm": {
    "overrides": {
      "tmp": "^0.2.3",
      "glob": ">=11.0.0",
      "rimraf": ">=6.0.0",
      "inflight": ">=2.0.0"
    }
  }
}
