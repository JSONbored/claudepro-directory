{
  "name": "claudepro-directory",
  "private": true,
  "version": "1.0.0",
  "description": "A community-driven directory of Claude configurations including agents, MCP servers, rules, commands, and hooks",
  "author": "JSONbored",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/claudepro/claudepro-directory"
  },
  "keywords": [
    "claude",
    "ai",
    "mcp",
    "agents",
    "configurations",
    "directory"
  ],
  "type": "module",
  "scripts": {
    "dev": "npm run build:content && vite",
    "build": "npm run build:content && npm run generate:sitemap && vite build",
    "build:analyze": "npm run build:content && npm run generate:sitemap && vite build --mode production && echo 'Bundle analysis available at dist/stats.html'",
    "build:content": "tsx scripts/build-content.ts",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "ultracite check",
    "format": "ultracite format --write",
    "test": "echo \"No tests configured yet\" && exit 0",
    "validate:content": "node scripts/validate-content.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.87.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.3.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.1",
    "react-syntax-highlighter": "15.6.6",
    "react-window": "^2.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react-swc": "4.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "ultracite": "^5.3.4",
    "vite": "7.1.5"
  }
}
