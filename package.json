{
  "name": "claudepro-directory",
  "private": true,
  "version": "1.0.0",
  "description": "A community-driven directory of Claude configurations including agents, MCP servers, rules, commands, and hooks",
  "author": "JSONbored",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JSONbored/claudepro-directory"
  },
  "keywords": [
    "claude",
    "ai",
    "mcp",
    "agents",
    "configurations",
    "directory"
  ],
  "type": "module",
  "scripts": {
    "dev": "npm run build:content && next dev --turbopack",
    "build": "npm run build:content && npm run generate:sitemap && npm run generate:static-apis && next build --turbopack",
    "build:analyze": "TURBOPACK_STATS=1 npm run build",
    "build:content": "tsx scripts/build-content.ts",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "generate:static-apis": "tsx scripts/generate-static-apis.ts",
    "generate:seo": "tsx scripts/seo/generate-all-seo.ts",
    "content:optimize": "tsx scripts/content-optimizer.ts",
    "content:optimize:file": "tsx scripts/content-optimizer.ts",
    "content:validate": "tsx scripts/content-optimizer.ts",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "ultracite check",
    "format": "ultracite fix",
    "validate:content": "tsx scripts/validate-content.ts",
    "ai:optimize": "tsx scripts/seo/shared/ai-format-optimizer.ts optimize",
    "ai:optimize:write": "tsx scripts/seo/shared/ai-format-optimizer.ts optimize --write",
    "ai:validate": "tsx scripts/seo/shared/ai-format-optimizer.ts validate",
    "ai:convert": "tsx scripts/seo/shared/ai-format-optimizer.ts convert",
    "ai:citation": "tsx scripts/seo/shared/ai-format-optimizer.ts citation",
    "freshness:report": "tsx scripts/seo/shared/content-freshness.ts report",
    "freshness:update": "tsx scripts/seo/shared/content-freshness.ts update",
    "freshness:queue": "tsx scripts/seo/shared/content-freshness.ts queue",
    "prepare": "command -v lefthook >/dev/null 2>&1 && lefthook install || echo 'Skipping lefthook install (not available)'"
  },
  "dependencies": {
    "@arcjet/next": "^1.0.0-beta.12",
    "@nosecone/next": "^1.0.0-beta.12",
    "@octokit/rest": "^22.0.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@upstash/redis": "^1.35.4",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.544.0",
    "marked": "^16.3.0",
    "next": "^15.5.3",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-window": "^2.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.4",
    "shiki": "^3.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "lefthook": "^1.13.5",
    "tailwindcss": "^4.1.13",
    "ultracite": "^5.4.6"
  }
}
