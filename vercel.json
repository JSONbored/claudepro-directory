{
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':(exclude).github/' ':(exclude)templates/' ':(exclude)*.md' ':(exclude)LICENSE' ':(exclude).gitignore'",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cache/warm",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/send-weekly-digest",
      "schedule": "0 14 * * 1"
    },
    {
      "path": "/api/cron/process-email-sequences",
      "schedule": "0 */4 * * *"
    }
  ]
}